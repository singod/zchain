body{min-width:990px} 

.hide {
	display: none
}

input {
	background: 0
}

.button,.tab-title li {
	background-image: url(i/btn.png);
	background-repeat: no-repeat
}

.feedback {
	height: 560px;
	background: url(i/bg1.png) no-repeat center 0;
	margin: 0;
	overflow: hidden
}

.feedback .mc {
	position: relative
}

.feedback .mc .phone {
	width: 310px;
	height: 220px;
	margin: 330px 0 0 650px;
	position: relative
}

.feedback .mc .phone input {
	border: 0;
	font-size: 14px;
	font-family: microsoft yahei
}

.feedback .mc .phone input.text {
	color: #000;
	width: 205px;
	line-height: 20px;
	height: 20px;
	display: block;
	z-index: 5;
	position: absolute;
	top: 165px;
	left: 0
}

.feedback .mc .phone input.button {
	color: #fff;
	cursor: pointer;
	width: 96px;
	height: 29px;
	line-height: 29px;
	text-align: center;
	background-position: 0 0;
	display: block;
	z-index: 5;
	position: absolute;
	top: 160px;
	left: 215px
}

.feedback .mc .phone input.button-gray {
	background-position: 0 -29px;
	color: #000
}

.feedback .mc .phone .tips {
	font-size: 12px;
	font-family: microsoft yahei;
	color: #f00;
	height: 25px;
	line-height: 25px;
	width: 310px;
	overflow: hidden;
	position: absolute;
	top: 190px;
	left: 0
}

.turntable {
	height: 662px;
	background: url(i/bg2.jpg) no-repeat center 0;
	margin: 0
}

.turntable .mc {
	position: relative
}

.turntable .mc .email {
	position: absolute;
	top: 8px;
	left: 611px;
	_left: 612px;
	width: 244px;
	height: 34px;
	background: url(i/email.jpg) no-repeat;
	text-align: center;
	color: #fff;
	font-family: microsoft yahei;
	font-size: 18px;
	line-height: 34px;
	cursor: pointer
}

.turntable .number {
	color: #1fd8eb;
	font-family: microsoft yahei;
	font-size: 30px;
	display: block;
	margin: 8px 0 0 412px;
	height: 30px;
	line-height: 30px;
	width: 45px;
	text-align: center
}

.turntable .market {
	width: 590px;
	float: left;
	height: 577px;
	margin: 15px 0 0 20px;
	_display: inline
}

.turntable .market .pointer {
	background: url(i/bg6.png) no-repeat;
	width: 177px;
	height: 257px;
	margin: 120px 0 0 196px;
	overflow: hidden
}

.turntable .market .pointer .btn {
	width: 170px;
	height: 160px;
	margin-top: 80px;
	cursor: pointer
}

.turntable .info {
	width: 345px;
	float: left;
	color: #fff;
	margin-top: 10px;
	font-size: 12px;
	font-family: microsoft yahei;
	padding: 0 0 0 15px
}

.turntable .info .product {
	margin-bottom: 110px;
	*margin-bottom: 98px
}

.turntable .info h2 {
	height: 60px;
	line-height: 60px;
	font-size: 16px;
	text-align: center
}

.turntable .info li {
	overflow: hidden;
	height: 26px;
	line-height: 26px;
	*height: 22px;
	*line-height: 22px
}

.turntable .info li .name {
	float: left;
	width: 285px;
	overflow: hidden;
	text-overflow: ellipsis;
	white-space: nowrap
}

.turntable .info li .extra {
	float: right;
	width: 50px;
	text-align: center
}

.turntable .info li .extra b {
	font-weight: normal
}

.turntable .info li .extra .light {
	color: #00eaff
}

.rule {
	height: 529px;
	background: url(i/bg3.jpg) no-repeat center 0;
	margin: 0
}

.rule .text {
	float: left;
	margin: 50px 0 0 25px;
	_margin: 50px 0 0 15px;
	width: 620px
}

.rule .text .item {
	overflow: hidden;
	clear: both
}

.rule .text b {
	display: block;
	float: left;
	width: 20px;
	height: 20px;
	line-height: 20px;
	text-align: center;
	color: #fff;
	background: #a21300;
	font-size: 16px;
	font-family: verdana;
	border-radius: 50%;
	font-weight: normal;
	margin-right: 10px;
	margin-top: 3px
}

.rule .text .con {
	float: left;
	font-size: 16px;
	font-family: microsoft yahei;
	width: 558px;
	line-height: 26px;
	color: #421611;
	letter-spacing: 2px
}

.rule .text .con .light {
	color: #a21300
}

.rule .affiche {
	float: left;
	width: 330px
}

.rule .affiche .tab-title {
	overflow: hidden;
	*zoom: 1;
	height: 80px
}

.rule .affiche .tab-title:before,.rule .affiche .tab-title:after {
	display: table;
	content: "";
	line-height: 0
}

.rule .affiche .tab-title:after {
	clear: both
}

.rule .affiche .tab-title li {
	width: 90px;
	height: 29px;
	line-height: 29px;
	text-align: center;
	font-size: 16px;
	color: #fff;
	font-family: microsoft yahei;
	float: left;
	margin-top: 22px;
	cursor: pointer;
	_display: inline;
	background-position: 0 -58px
}

.rule .affiche .tab-title li.fore1 {
	margin-left: 37px
}

.rule .affiche .tab-title li.fore2 {
	margin-left: 79px
}

.rule .affiche .tab-title li.gray {
	background-position: 0 -87px;
	color: #000
}

.rule .affiche .tab {
	font-family: microsoft yahei;
	font-size: 12px;
	color: #000;
	padding: 0 0 0 5px
}

.rule .affiche .tab li {
	line-height: 24px;
	height: 24px;
	overflow: hidden;
	*height: 19px;
	*line-height: 19px;
	*zoom: 1;
	float: none
}

.rule .affiche .tab li:before,.rule .affiche .tab li:after {
	display: table;
	content: "";
	line-height: 0
}

.rule .affiche .tab li:after {
	clear: both
}

.rule .affiche .tab li span {
	display: block;
	float: left
}

.rule .affiche .tab li span.list1 {
	width: 80px
}

.rule .affiche .tab li span.list2 {
	width: 135px
}

.rule .affiche .tab li span.list3 {
	width: 110px;
	_width: 100px
}

.advert {
	height: 382px;
	background: url(i/bg4.jpg) no-repeat center 0;
	margin: 0;
	padding: 40px 0 0
}

.advert .listwrap {
	width: 990px
}

.advert .listwrap li {
	float: left;
	border: 1px solid #a60000;
	background: #fff;
	width: 312px;
	height: 146px;
	margin: 0 0 10px 13.5px;
	_display: inline;
	overflow:hidden; 	
}

.layer {
	background: url(i/bg5.jpg) no-repeat;
	width: 362px;
	height: 179px
}

.layer .con {
	position: relative;
	overflow: hidden;
	width: 362px;
	height: 179px
}

.layer .con .text {
	position: absolute;
	top: 10px;
	left: 7px;
	color: #fff;
	font-size: 16px;
	font-family: microsoft yahei
}

.layer .con .shut {
	width: 16px;
	height: 16px;
	display: block;
	position: absolute;
	top: 12px;
	right: 14px
}

.layer .con p {
	font-size: 18px;
	color: #fff;
	font-family: microsoft yahei;
	text-align: center
}

.layer .con p.fore1 {
	margin: 50px 0 10px 0
}

.layer .con .button {
	width: 87px;
	height: 30px;
	line-height: 30px;
	text-align: center;
	color: #fff;
	font-size: 16px;
	font-family: microsoft yahei;
	background-position: 0 -58px;
	cursor: pointer;
	margin: 20px 20px 0 110px;
	float: left
}

.layer .con .sina {
	display: block;
	float: left;
	width: 40px;
	height: 30px;
	background-position: 0 -145px;
	text-indent: -9999px;
	margin: 20px 0 0 0
}

.layer2 .con p.fore1 {
	margin-top: 60px;
	font-size: 16px
}

.layer3 .con p.fore1 {
	font-size: 14px;
	margin-top: 50px
}

.layer3 .con .item {
	padding-left: 25px;
	margin: 10px 0 10px 0;
	overflow: hidden
}

.layer3 .con .item input {
	border: 1px solid #767676
}

.layer3 .con .item input.inputEmail {
	width: 150px;
	background: #fff
}
.layer3 .con .item input.name {
	width: 100px;
	background: #fff
}

.layer3 .con .item input.phone {
	width: 110px;
	background: #fff
}

.layer3 .con .item input.address {
	width: 180px;
	background: #fff
}

.layer3 .con .item input.button {
	width: 67px;
	background-position: 0 -116px;
	border: 0;
	margin: 0 0 0 10px;
	float: none;
	_vertical-align: middle
}

.layer3 .con .item span {
	font-size: 14px;
	font-family: microsoft yahei;
	color: #fff
}